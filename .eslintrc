{
  "extends": ["eslint:recommended", "google"],
  "rules": {
    "no-var": 2,
    "no-console": 0,
    "comma-dangle": 0,
    "require-jsdoc": 0,
    "no-useless-escape": 0,
    "max-len": ["error", 120]
  },
  "parserOptions": {
    "ecmaVersion": 8
  },
  "globals": {
    "Msal": true,
    "graphAPIScopes": true
  }
}
